{
  "env": {
    "jest": true
  },
  "extends": ["airbnb-base", "prettier"],
  "globals": {
    "runFormatTest": false
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": "off",
    "import/prefer-default-export": "off",
    "class-methods-use-this": "off"
  }
}
